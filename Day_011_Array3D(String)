package PERCABANGAN;

public class Day_008_Array3D_A {

    public static void main(String[] args) {
        String nama[][][] = {{{"Naruto", "Sasuke", "Eren", "Goku", "Anos", "Levi"}}};
        for (int i = 0; i <nama.length; i++) {
            for (int j = 0; j < nama[i].length; j++) {
                for (int k = 0; k < nama[i][j].length; k++) {
                    System.out.println(nama[i][j][k]);
                }
 
            }
            System.out.println("");
        }

    }
}
